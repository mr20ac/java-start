package com.company;

import java.util.Scanner;

public class Main {
    static void oa(int amount, String loop) {

        for(int i=0; i<amount;i++) {
            System.out.println(loop);
        }
    }
    public static void main(String[] args) {
	Scanner key = new Scanner(System.in);
    System.out.println("enter loop content");
    String loop = key.nextLine();
    System.out.println("enter loop amount");
        int amount = key.nextInt();
        oa(amount,loop);
    }
}
